<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.firstdevelop.boot.mapper.EmployeeMapper">
    <select id="searchAll" resultType="com.firstdevelop.boot.entity.Employee">
        SELECT
		    id,
		    name,
		    age,
		    salary,
		    department
		FROM
		    employee
    </select>
</mapper>